{
    "env": {
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
      "rules": {
    "react/jsx-filename-extension": ["error", [".js", ".jsx"]],
    "import/prefer-default-export": "off"
  }
}

